{
  "name": "@steadysass/app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f ../../.env -- next dev",
    "prebuild": "pnpm run generate",
    "build": "next build",
    "generate": "pnpm run generate:prepare-generated-folder && pnpm run generate:typed-graphql-builder && pnpm run generate:codegen && pnpm run generate:append-generated-typed-graphql-builder",
    "generate:prepare-generated-folder": "mkdir -p src/generated",
    "generate:typed-graphql-builder": "npx typed-graphql-builder --schema ../graphql/src/generated/schema.graphql --output ./src/generated/typed-graphql-builder.ts --includeTypename=true --scalar DateTime=string --scalar Time=string",
    "generate:append-generated-typed-graphql-builder": "ts-node ./src/generators/append-generated-typed-graphql-builder.ts",
    "generate:codegen": "npx graphql-codegen",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.10.6",
    "@fluentui/react-icons": "^2.0.274",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@graphql-yoga/node": "^3.9.1",
    "@hookform/resolvers": "^3.10.0",
    "@steadysass/graphql": "workspace:*",
    "@steadysass/routes": "workspace:*",
    "@steadysass/validations": "workspace:*",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.8.0",
    "json2ts": "^0.0.7",
    "next": "15.1.6",
    "quicktype": "^23.0.171",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.3.1",
    "ts-pattern": "^5.6.2",
    "urql": "^4.2.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/typescript": "^4.1.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
